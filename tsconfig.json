{
  "compilerOptions":
  {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "./build",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowJs": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
  },
"include": ["src/**/*"]
}
